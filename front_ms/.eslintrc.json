{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:import/errors"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "import", "jsx-a11y"],
  "rules": {
    "react/jsx-uses-vars": "warn",
    "react/jsx-uses-react": "warn",
    "no-unused-vars": "off",
    "react/prop-types": 0,
    "react/jsx-key": 0
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  }
}
